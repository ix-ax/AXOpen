{
  "metadata": [
    {
      "src":  [
        {
          "files": [
            "abstractions/src/**.csproj",
            "base/src/**.csproj",
            "core/src/**.csproj",
            "data/src/**.csproj",
            "inspectors/src/**.csproj",
            "components.cognex.vision/src/**.csproj",
            "components.pneumatics/src/**.csproj",
            "components.elements/src/**.csproj"
          ],
          "exclude": [
            "**Test*.csproj",
            "**/bin/**",
            "**/obj/**",
            "*.Properties",
            "*.Resx",
            "*.g.cs"
          ],
          "src": "../src/"
        }
      ],            
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore" : true,
      "properties": {
        "TargetFramework": "net7.0"
      }            
    }
  ],
  "build": {
    "globalMetadata" : {
        "_appTitle" : {"api/index.md" : "API"},
        "_appName" : "",
        "_appFooter" : " Generated by DocFx. Â© PTKu, MTS spol. s r.o., and awesome contributors",
        "_enableSearch" : true,
        "_appFaviconPath" : "images/favicon.ico",    
        "_appLogoPath" : "images/logo.svg"   
    },
    
    "sitemap": {
      "baseUrl": "https://ix-ax.github.io/AXOpen/",
      "priority": 0.1,
      "changefreq": "weekly"
    },

    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "apictrl/**.yml",
          "apictrl/index.md",
          "apidoc/**.yml",
          "apidoc/index.md"
        ]       
      },      
      {
        "files": [
          "articles/**.md",
          "articles/**.png",
          "articles/**.svg",
          "articles/**.gif",
          "articles/**/toc.yml",        
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "components/**.md",
          "components/**.png",
          "components/**.svg",
          "components/**.gif",
          "components/**/toc.yml",        
          "toc.yml",
          "*.md"
        ]
      },
      {
      "files": [
        "framework/**.md",
        "framework/**.png",
        "framework/**.svg",
        "framework/**.gif",
        "framework/**/toc.yml",        
        "toc.yml",
        "*.md"
      ]
      },          
      {
        "files": [            
            "**/**.md",
            "**/**.png",
            "**/**.svg",
            "**/**.gif",            
            "**/**/toc.yml",
            "toc.yml",                    
            "*.md"
        ],
        "src": "../src/"        
      }      
    ],
    "resource": [
      {
        "files": [          
          "**/**.png",
          "**/**.svg",
          "**/**.gif"    
        ],
        "src" : "../src/"           
      }            
    ],    
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", 
                  "modern", 
                  "templates/material/material",
                  "templates/custom"
                ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}