@page "/ui/CuAllAuto"
@using AXOpen.Messaging.Static;
@using sandboxtest.hmi.Components;
@inherits RenderableComponentBase;



@{
    int tInterval = 100;

    @foreach (var CuInstance in Entry.Plc.Context.GetChildren().OfType<sandboxtest.StandardControlUnit.CU>().ToList())
    {
        <UnitsSequences Component="@CuInstance" PollingInterval="tInterval"></UnitsSequences>
        tInterval = tInterval + 150;
    }

}