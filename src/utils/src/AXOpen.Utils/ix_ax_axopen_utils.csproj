    <PackageId>AXOpen.Utils</PackageId>
    <RootNamespace>AXOpen.Utils</RootNamespace>
  </PropertyGroup>

	<ItemGroup>
    <PackageReference Include="GitVersion.MsBuild" Version="5.10.3">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="AXSharp.Abstractions" Version="0.14.0-alpha.97" />
		<PackageReference Include="AXSharp.Connector" Version="0.14.0-alpha.97" />
