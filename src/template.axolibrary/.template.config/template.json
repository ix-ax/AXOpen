{
  "$schema": "http://json.schemastore.org/template",
  "author": "PTKu",
  "classifications": [ "AXOpen", "axolibrary" ],
  "identity": "librarytemplate",
  "name": "AXOpen Library Template",
  "shortName": "axolibrary",
  "tags": {
    "language": "C#",
    "type": "project"
  },  
  "sourceName": "librarytemplate",
  "symbols": {
    "axlibname": {
      "type": "parameter",
      "description": "AX library project name",
      "datatype": "text",
      "defaultValue": "axolib",
      "replaces": "axolib",
      "fileRename" : "axolib"
    },
    "projname": {
      "type": "parameter",
      "description": ".NET name",
      "datatype": "text",
      "defaultValue": "dotnetlibs",
      "replaces": "dotnetlibs",
      "fileRename" : "dotnetlibs"
    },
    "nameLower":{
      "type": "generated",
      "generator": "casing",
      "parameters": {
        "source":"axlibname",
        "toLower": true
      },
      "nameUpper":{
        "type": "generated",
        "generator": "casing",
        "parameters": {
          "source":"projname",
          "toLower": false
      }
    }
  }  
}
}
  